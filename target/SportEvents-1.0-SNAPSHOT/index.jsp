</header>

<section class="box-results">

    <form class="input-group" style="width:500px; margin:auto; padding:20px" action="FindServlet">
        <input type="search" class="form-control rounded" placeholder="Search" aria-label="Search" name="title" value="${title}"/>
        <button type="submit" class="btn btn-outline-primary">search</button>
    </form>

    <div class="container">
        <div class="row ">

            <div class="col-lg-4">
                <h4>Tournaments</h4>
                <div class="info-ranking">
                    <ul style="height:400px;overflow-y:auto">

                            <c:forEach var="competition" items="${allCompetitions}">
                            <li>
                                <form action="CompetitionServlet" method="post">
                                    <input type="hidden" name="id" value="${competition.getCompId()}">
                                    <input class="submit-all" style="background:blue; " type="submit" value="${competition.getTitle()}">
                                </form>
                            </li>
                            </c:forEach>

                    </ul>
                </div>
            </div>

            <div class="col-lg-4">
                <h4>${comp.getTitle()} Matches</h4>
                <div class="info-results info-ranking">
                    <ul style="height:400px;overflow-y:auto">
                        <c:forEach var="game" items="${comp.getGames()}">
                        <li>
                      <span class="head">
                        ${game.getTitle()}<span class="date">${game.getDate()}</span>
                      </span>
                            <div class="goals-result">
                                    ${game.getTeam1().getTitle()}
                                <span class="goals">
                          <b>${game.getResult()}</b>
                        </span>
                                    ${game.getTeam2().getTitle()}
                            </div>
                        </li>
                        </c:forEach>
                    </ul>
                </div>
            </div>
            <div class="col-lg-4 ">
                <h4>Participants</h4>
                <div class="info-ranking">
                    <ul style="height:400px;overflow-y:auto">
                        <c:forEach var="team" items="${comp.getTeams()}" varStatus="counter">
                        <li>
                            <span class="position">${counter.count}</span>
                            ${team.getTitle()}
                        </li>
                        </c:forEach>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <h2 style="text-align:center; color:gold" >Winner:</h2>
    <h5 style="text-align:center;">${comp.getResult()}</h5>
</section>

<footer>
    <div class="container">
        <div class="row">
            <div class="col text-center">
                © Brazhnik Olexandr.
            </div>
        </div>
    </div>
</footer>



